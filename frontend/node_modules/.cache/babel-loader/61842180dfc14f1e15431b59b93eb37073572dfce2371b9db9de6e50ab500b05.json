is.email,\n        host: this.host,\n        database: this.database,\n        user: this.user,\n        password: this.password,\n      }\n      console.log(\"Payload: \", JSON.stringify(payload));\n\n      const requestOptions = {\n        method: 'POST',\n        headers: {'Content-Type': 'application/json'},\n        body: JSON.stringify(payload)\n      };\n\n      fetch('http://localhost:8081/users/login', requestOptions)\n          .then(response => response.json())\n          .then(data => {\n            if (data.error) {\n              console.log(\"Error: \", data.message);\n              notie.alert({type: 'error', text: data.message, time: 2});\n            } else {\n              console.log(\"Success: \", data.message);\n              notie.alert({type: 'success', text: data.message, time: 2});\n              this.$store.commit('setCredentials', payload);\n              // this.$router.push({name: 'Home'});\n            }\n          })\n    }\n  }\n}\n</script>"],"mappings":"AAyDA,OAAOA,SAAQ,MAAO,uBAAsB;AAC5C,OAAOC,OAAM,MAAO,qBAAoB;AACxC,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVJ,SAAS;IACTC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACdC,OAAO,CAACC,GAAG,CAAC,iCAAiC,CAAC;MAE9C,MAAMC,OAAM,GAAI;QACdT,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,QAAQ,EAAE,IAAI,CAACA;MACjB;MACAG,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEE,IAAI,CAACC,SAAS,CAACF,OAAO,CAAC,CAAC;MAEjD,MAAMG,cAAa,GAAI;QACrBC,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UAAC,cAAc,EAAE;QAAkB,CAAC;QAC7CC,IAAI,EAAEL,IAAI,CAACC,SAAS,CAACF,OAAO;MAC9B,CAAC;MAEDO,KAAK,CAAC,mCAAmC,EAAEJ,cAAc,EACpDK,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAAClB,IAAG,IAAK;QACZ,IAAIA,IAAI,CAACqB,KAAK,EAAE;UACdb,OAAO,CAACC,GAAG,CAAC,SAAS,EAAET,IAAI,CAACsB,OAAO,CAAC